# Environment Configuration for SAP WhatsApp Integration

# SAP Business One Service Layer Configuration
VITE_SAP_DATABASE=your_database_name
VITE_SAP_USERNAME=your_sap_username
VITE_SAP_PASSWORD=your_sap_password

# WhatsApp Configuration
WHATSAPP_SESSION_PATH=./whatsapp-session
WHATSAPP_ENABLED=true

# Invoice Configuration
INVOICE_CHECK_INTERVAL=3600000
INVOICE_PDF_PATH=./invoices
COMPANY_NAME=Your Company Name

# Notification Settings
ADMIN_PHONE=549xxxxxxxxx
ENABLE_ADMIN_NOTIFICATIONS=true

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/whatsapp-service.log
